            "column": 1,
            "row": 1,
            "height": 3,
            "width": 2
          },
          "title": "",
          "rawConfiguration": {
            "text": "# FoodMe Checkout\n## Bottom of the Funnel Analysis\n"
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.funnel"
          },
          "layout": {
            "column": 3,
            "row": 1,
            "height": 3,
            "width": 6
          },
          "title": "Conversion Funnel",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "accountId": 192626,
                "query": "SELECT funnel(session, WHERE actionText LIKE 'Restaurant' as 'Select Restaurant', WHERE actionText LIKE 'Menu' as 'Select Meal', WHERE actionText LIKE 'Checkout' as 'Checkout', WHERE actionText LIKE 'Thank you' as 'Order Complete') FROM BrowserInteraction SINCE '2021-10-11 00:38:00 -0700'"
              }
            ]
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 9,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Failed Conversion Checks",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 192626,
                "query": "FROM SyntheticCheck select percentage(count(*),WHERE result = 'FAILED') where monitorName = 'Food App' SINCE 1 WEEK AGO TIMESERIES AUTO"
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 1,
            "row": 4,
            "height": 2,
            "width": 6
          },
          "title": "Revenue at Risk - Latency",
          "rawConfiguration": {
            "dataFormatters": [],
            "nrqlQueries": [
              {
                "accountId": 192626,
                "query": "SELECT count(*) * (6.2) AS USD FROM PageView, AjaxRequest  WHERE (requestUrl LIKE '%checkout%' OR requestUrl LIKE '%thank-you%') AND  (timeToSettle > 0.05 OR backendDuration > 0.5) SINCE 1 WEEK AGO compare with 1 WEEK ago"
              }
            ],
            "thresholds": []
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 7,
            "row": 4,
            "height": 2,
            "width": 6
          },
          "title": "Revenue at Risk - Errors",
          "rawConfiguration": {
            "dataFormatters": [],
            "nrqlQueries": [
              {
                "accountId": 192626,
                "query": "FROM Transaction SELECT count(*) * 6.2 AS USD WHERE http.statusCode >= '300' AND (request.uri LIKE '%checkout.html%' OR request.uri LIKE '%thank-you.html%') SINCE 1 WEEK AGO COMPARE WITH 1 WEEK AGO"
              }
            ],
            "thresholds": []
          },
          "linkedEntityGuids": null
        },
